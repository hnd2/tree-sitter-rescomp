{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "rescomp",
      "camelcase": "Rescomp",
      "title": "Rescomp",
      "scope": "source.rescomp",
      "file-types": ["res"],
      "injection-regex": "^rescomp$",
      "class-name": "TreeSitterRescomp",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Rescomp grammar for tree-sitter",
    "authors": [
      {
        "name": "hnd"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-rescomp"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
