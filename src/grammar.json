{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "rescomp",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_statement"
      }
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "bitmap_expression"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "string_literal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^\"\\\\]"
              },
              {
                "type": "PATTERN",
                "value": "\\\\."
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "bitmap_compression": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "STRING",
            "value": "-1"
          },
          {
            "type": "STRING",
            "value": "0"
          },
          {
            "type": "STRING",
            "value": "1"
          },
          {
            "type": "STRING",
            "value": "2"
          },
          {
            "type": "STRING",
            "value": "AUTO"
          },
          {
            "type": "STRING",
            "value": "BEST"
          },
          {
            "type": "STRING",
            "value": "NONE"
          },
          {
            "type": "STRING",
            "value": "APLIB"
          },
          {
            "type": "STRING",
            "value": "FAST"
          },
          {
            "type": "STRING",
            "value": "LZ4W"
          }
        ]
      }
    },
    "bitmap_expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "BITMAP"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "string_literal"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "bitmap_compression"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}